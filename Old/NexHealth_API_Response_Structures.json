          "location_id": 333724,
          "available": true
        },
        {
          "start_time": "2025-01-20T10:00:00.000-05:00",
          "end_time": "2025-01-20T10:30:00.000-05:00",
          "provider_id": 421458314,
          "provider_name": "Dr. Smith",
