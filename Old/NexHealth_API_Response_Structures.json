  "JLTEST_Patient_Lookup-CustomTool": {
    "description": "Response from searching for patients by date of birth",
    "endpoint": "GET https://nexhealth.info/patients?date_of_birth=YYYY-MM-DD",
    "example_response": {
      "data": [
        {
          "id": 425105043,
          "first_name": "John",
          "last_name": "Doe",
          "date_of_birth": "2010-05-15",
          "phone": "314-303-5067",
          "email": "john.doe@example.com",
          "address": {
            "street": "123 Main St",
            "city": "Philadelphia",
            "state": "PA",
            "zip": "19101"
          },
          "created_at": "2020-01-15T10:30:00.000-05:00",
          "updated_at": "2024-12-01T14:20:00.000-05:00"
        }
      ],
      "count": 1
    },
    "fields": [
      "data (array of patient objects)",
      "count",
      "Each patient object contains:",
      "  - id",
      "  - first_name",
      "  - last_name",
      "  - date_of_birth",
      "  - phone",
      "  - email",
      "  - address (object)",
      "  - created_at",
      "  - updated_at"
    ],
    "note": "Returns an array of patients matching the DOB. May return empty array if no matches found."
  },
